generator client { provider = "prisma-client-js" }

datasource db {
  provider = env("DATABASE_PROVIDER") // "postgresql" or "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  name      String?
  email     String   @unique
  image     String?
  createdAt DateTime @default(now())
}